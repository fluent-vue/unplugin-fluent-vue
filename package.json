{
  "name": "unplugin-fluent-vue",
  "description": "unplugin for fluent-vue that adds support for custom blocks in Vue SFC",
  "author": {
    "name": "Ivan Demchuk",
    "email": "ivan.demchuk@gmail.com",
    "url": "https://demivan.me"
  },
  "license": "MIT",
  "keywords": ["vue", "fluent-vue", "webpack", "vite", "i18n"],
  "exports": {
    "production": {
      "require": "./dist/prod/index.js",
      "default": "./dist/prod/index.mjs"
    },
    "require": "./dist/index.js",
    "default": "./dist/index.mjs"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@vue/compiler-sfc": "^3.2.37",
    "debug": "^4.3.4",
    "unplugin": "^0.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.25.2",
    "@fluent/bundle": "^0.17.1",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "memfs": "^3.4.7",
    "release-it": "^14.14.3",
    "release-it-lerna-changelog": "^4.0.1",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vitest": "^0.16.0",
    "vue": "^3.2.37",
    "vue-loader": "^17.0.0",
    "webpack": "^5.73.0"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.ts?(x)": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu/eslint-config-ts"
    ]
  }
}
