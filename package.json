{
  "name": "unplugin-fluent-vue",
  "version": "1.0.0",
  "description": "unplugin for fluent-vue. Adds support for custom blocks in Vue SFC and external ftl files.",
  "author": {
    "name": "Ivan Demchuk",
    "email": "ivan.demchuk@gmail.com",
    "url": "https://demivan.me"
  },
  "license": "MIT",
  "homepage": "https://fluent-vue.demivan.me/integrations/unplugin.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluent-vue/unplugin-fluent-vue.git"
  },
  "keywords": [
    "unplugin",
    "vue",
    "fluent-vue",
    "ftl",
    "fluent",
    "webpack",
    "vite",
    "i18n",
    "plural"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest  --coverage",
    "build": "tsup",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fluent/syntax": "^0.18.1",
    "@rollup/pluginutils": "^4.2.1",
    "magic-string": "^0.26.2",
    "unplugin": "^0.9.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "0.26.3",
    "@release-it-plugins/lerna-changelog": "5.0.0",
    "@types/node": "18.6.1",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitest/coverage-c8": "0.23.2",
    "@vue/compiler-sfc": "3.2.39",
    "eslint": "8.23.1",
    "execa": "6.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "memfs": "3.4.7",
    "release-it": "15.4.2",
    "tsup": "6.2.3",
    "typescript": "4.8.3",
    "vite": "3.1.2",
    "vitest": "0.23.2",
    "vue": "3.2.39",
    "vue-loader": "17.0.0",
    "webpack": "5.74.0"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.ts?(x)": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu/eslint-config-ts"
    ]
  },
  "changelog": {
    "labels": {
      "Type: Feature": ":star: New Features",
      "Type: Bug": ":bug: Bug Fixes",
      "Type: Security": ":lock: Security Fixes",
      "Type: Performance": ":chart_with_upwards_trend: Performance Fixes",
      "Type: Improvement": ":zap: Improved Features",
      "Type: Breaking": ":boom: Breaking Change",
      "Type: Deprecated": ":warning: Deprecated Features",
      "Type: I18n": ":globe_with_meridians: Internationalization",
      "Type: A11y": ":wheelchair: Accessibility",
      "Type: Documentation": ":pencil: Documentation"
    }
  }
}
